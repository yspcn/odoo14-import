<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sh_moq_multi_website_res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.moq.website.form.inherit</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="website_sale.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//div//div[@id='sale_product_catalog_settings']" position="after">
                <h2>Multiple Qty for Multi Website</h2>
                <div class="row mt16 o_settings_container">
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane">
                            <field name="multi_website_moq" />
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="multi_website_moq" />
                            <div class="text-muted"></div>
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>
